<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

 <Menu xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EditMenuController" mnemonicParsing="false" text="編集(E)">
    <items>
         <MenuItem mnemonicParsing="false" text="元に戻す(U)...               Ctrl+Z" />
         <MenuItem mnemonicParsing="false" onAction="#onCutText" text="切り取り(T)                    Ctrl+X" />
         <MenuItem mnemonicParsing="false" onAction="#onCopyText" text="コピー(C)                     Ctrl+C" />
         <MenuItem mnemonicParsing="false" onAction="#onPasteText" text="貼り付け(P)                 Ctrl+V" />
         <MenuItem mnemonicParsing="false" onAction="#onDeleteText" text="削除(L)                          Del" />
         <MenuItem mnemonicParsing="false" onAction="#onOpenSearch" text="検索(F)...                 Ctrl+F" />
         <MenuItem mnemonicParsing="false" text="次を検索(N)                  F3" />
         <MenuItem mnemonicParsing="false" text="置換(R)...                  Ctrl+H" />
         <MenuItem mnemonicParsing="false" onAction="#onMoveToLine" text="行へ移動(G)...               Ctrl+G" />
         <MenuItem mnemonicParsing="false" onAction="#onSelectAllText" text="すべて選択(A)                 Ctrl+A" />
         <MenuItem mnemonicParsing="false" onAction="#onInsertDateTime" text="日付と時刻(D)                  F5" />
    </items>
 </Menu>
